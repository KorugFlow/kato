func main() {
    var int a = inpt("Enter a number 1 : ");
    var int b = inpt("Enter a number 2 : ");
    var char c = inpt("Enter a op : ");
    switch (c) {
        case '+'
            print(*a* + *b*);
            break;
        case '-'
            print(*a* - *b*);
            break;
        case '*'
            print(*a* * *b*);
            break;
        case '/'
            print(*a* / *b*);
            break;
    }
    return 0;
}